---
layout: layout.njk
---

<div class="post-list">
    <div class="page-content" style="margin-bottom: 40px; text-align: center;">
        {{ content | safe }}
    </div>
    
    {% for post in collections.gapyear %}
    <article>
        <h2><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
        <div class="date">{{ post.date | dateFormat }} Â· {{ post.templateContent | readingTime }} min read</div>
    </article>
    {% endfor %}
</div>
